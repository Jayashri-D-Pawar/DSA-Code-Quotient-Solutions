/* add c headers if necessary*/
#include <stdio.h>
/* Include other headers as needed */
int main()
{
   int year ;
  scanf("%d",&year);
  if(year %400 ==0 || year %4 ==0 && year %100 !=0){
    printf("Leap Year");
  }else{
    printf("Not a Leap Year");
  }
    return 0;
}
/* add tail for c if necessary*/
